{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "projectspotify"
		},
		"Spotify_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Spotify_ADLS'"
		},
		"Spotify_SqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Spotify_SqlDatabase'"
		},
		"Spotify_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyproject.dfs.core.windows.net/"
		},
		"Spotify_SqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "projectspotify.database.windows.net"
		},
		"Spotify_SqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyproject"
		},
		"Spotify_SqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Lokesh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "projectspotify_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Spotify_ADLS_accountKey": {
						"value": "[parameters('Spotify_ADLS_accountKey')]"
					},
					"Spotify_SqlDatabase_password": {
						"value": "[parameters('Spotify_SqlDatabase_password')]"
					},
					"Spotify_ADLS_properties_typeProperties_url": {
						"value": "[parameters('Spotify_ADLS_properties_typeProperties_url')]"
					},
					"Spotify_SqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('Spotify_SqlDatabase_properties_typeProperties_server')]"
					},
					"Spotify_SqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('Spotify_SqlDatabase_properties_typeProperties_database')]"
					},
					"Spotify_SqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('Spotify_SqlDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}